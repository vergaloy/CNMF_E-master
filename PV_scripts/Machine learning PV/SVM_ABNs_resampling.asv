function out=SVM_ABNs_resampling(D)


sim=10;

out=cell(2,5);

for i=1:sim
    temp=SVM_ABNs_matrix(D);
    out=arrange_matrix(out,temp);
end    
    
dummy=1;    
end

function out=arrange_matrix(out,temp)

for i=1:5
    out{1,i}=cat(3,out{1,i},temp{1,i});
    if (~isempty(out{2,i}))
    out{2,i}=out{2,i}+temp{2,i};
    else
      out{2,i}=temp{2,i};   
    end
end

end
    